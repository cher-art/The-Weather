{{#each this}}
<li class="item">
  <div>
    <video controls="controls" src="{{videos.tiny.url}}" height="400" width="800"></video>
  </div>
  <div>
    <p>Количество коментариев: {{comments}}</p>
    <p>Количество загрузок: {{downloads}}</p>
    <p>Продолжительность видио: {{duration}}</p>
    <p>Количество лайков: {{likes}}</p>
    <p>Количество просмотров: {{views}}</p>
  </div>
</li>
{{/each}}